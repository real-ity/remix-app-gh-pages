{
  "compilerOptions": {
    "jsx": "react-jsx",
    "lib": [
      "dom",
      "dom.iterable",
      "dom.asynciterable",
      "deno.ns"
    ],
    "paths": {
      "~/*": [
        "./app/*"
      ],
      "~/theme": [
        "./app/theme/index.ts"
      ]
    },
    "strict": true,
    "types": [
      "@remix-run/deno",
      "vite/client"
    ]
  },
  "exclude": [
    ".github/",
    ".vscode/",
    "build/"
  ],
  "imports": {
    "~/": "./app/",
    "~/theme": "./app/theme/index.ts"
  },
  "nodeModulesDir": "auto"
}