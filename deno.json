{
  "compilerOptions": {
    "jsx": "react-jsx",
    "lib": [
      "dom",
      "dom.iterable",
      "dom.asynciterable",
      "deno.ns"
    ],
    "paths": {
      "~/*": [
        "./app/*"
      ],
      "~/theme": [
        "./app/theme/index.ts"
      ]
    },
    "strict": true,
    "types": [
      "@react-router/node",
      "vite/client"
    ]
  },
  "exclude": [
    ".devcontainer/",
    ".github/",
    ".vscode/",
    "build/"
  ],
  "include": [
    "**/*.ts",
    "**/*.tsx",
    "**/.server/**/*.ts",
    "**/.server/**/*.tsx",
    "**/.client/**/*.ts",
    "**/.client/**/*.tsx",
    ".react-router/types/**/*"
  ],
  "imports": {
    "~/": "./app/",
    "~/theme": "./app/theme/index.ts"
  },
  "nodeModulesDir": "auto"
}
